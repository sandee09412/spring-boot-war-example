pipeline{
  agent any
 stages{
   stage ('build') {
    steps{
      echo "Building the project"
      }
     }
    }
  post {
   success {
     emailext attachLog: true, body: 'Email sent out from Jenkins', subject: '$PROJECT_NAME - Build # $BUILD_NUMBER - $BUILD_STATUS!', to: 'prabhakarq3@gmail.com'
     }
   failure {
     emailext attachLog: true, body: 'Email sent out from Jenkins', subject: '$PROJECT_NAME - Build # $BUILD_NUMBER - $BUILD_STATUS!', to: 'prabhakarq3@gmail.com'
   }
  }
 }
    
